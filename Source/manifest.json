{
  "manifest_version": 2,
  "name": "Trump Dumper",
  "version": "1.4",
  "description": "Replace all mention of Donald J. Trump with an insulting epithet.",
  "icons": { "16": "trump-dumper-16.png", "128": "trump-dumper-128.png" },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
